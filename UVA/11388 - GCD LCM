#include<stdio.h>
#include<bits/stdc++.h>
#define ll long long
#define PI acos(-1)
using namespace std;

int main()
{

    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    ll t,i,g,l;
    cin>>t;
    while(t--){
        cin>>g>>l;
        if(l%g==0){
            cout<<g<<" "<<l<<endl;
        }
        else
            cout<<"-1"<<endl;
    }
return 0;
}
